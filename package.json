{
  "name": "holmersson-frontend-with-react",
  "version": "1.2.0",
  "private": true,
  "dependencies": {
    "bootstrap": "3.3.7",
    "chartist": "0.10.1",
    "crypto-js": "^3.1.9-1",
    "markdown-react-js": "^1.0.2",
    "marked": "^0.6.0",
    "mongodb": "^3.1.13",
    "nl2br": "0.0.3",
    "npm-run-all": "4.1.2",
    "react": "16.2.0",
    "react-bootstrap": "0.32.1",
    "react-chartist": "0.13.1",
    "react-dom": "16.2.0",
    "react-notification-system": "0.2.17",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.1.1",
    "reactjs-localstorage": "0.0.7"
  },
  "devDependencies": {
    "http-server": "^0.11.1",
    "mocha": "^4.1.0",
    "selenium-webdriver": "^3.6.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "react-scripts start",
    "_comment_start": "npm-run-all -p watch-css start-js",
    "build": "react-scripts build",
    "_test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start-test": "node node_modules/http-server/bin/http-server -p 8082 src",
    "test": "mocha"
  }
}
